{
	"manifest_version": 2,

	"name": "New Tab",
	"description": "better than brave",
	"version": "0.2",

	"chrome_url_overrides" : {
		"newtab": "startpg.html"
	},

	"browser_action": {
		"default_icon": "icon.png"
	},

	"permissions":[
		"tabs"
	],

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}
